:"//Store Information for BotInfo Command\nvar Discord = require(\"discord.js\");\n\n//About the Bot\nthis.storeValue(client.user.displayAvatarURL(), 1, \"bAvatar\", cache);\nthis.storeValue(client.user.username, 1, \"bName\", cache);\nthis.storeValue(client.nickname, 1, \"bNick\", cache);\nthis.storeValue(client.guilds.cache.array(), 1, \"guildList\", cache);\nthis.storeValue(client.user.tag, 1, \"bTag\", cache);\nthis.storeValue(client.user.id, 1, \"bID\", cache);\n\nvar bTime = client.user.createdAt.toLocaleString('en-US', {timeZone: 'America/New_York', timeZoneName: 'short', month: 'short', day: '2-digit', year: 'numeric'});\nthis.storeValue(bTime, 1, \"bTime\", cache);\n\n//Technical Info\n//store bots OS\nswitch(process.platform){\n\tcase \"win32\": var os = \"Windows\"; break;\n\tcase \"linux\": var os = \"Linux\"; break;\n\tcase \"darwin\": var os = \"Darwin\"; break;\n\tcase \"openbsd\": var os = \"OpenBSD\"; break;\n\tcase \"sunos\": var os = \"Solaris\"; break;\n\tcase \"freebst\": var os = \"FreeBSD\"; break;\n};\nthis.storeValue(os, 1, \"os\", cache);\n\nthis.storeValue(Discord.version, 1, \"dVersion\", cache);\n\n//General Info\nthis.storeValue(client.user.presence.status, 1, \"bStatus\", cache);\n\nif (!game) {\n\tthis.storeValue(\"No Status\", 1, \"bType\", cache);\n\tthis.storeValue(\"n/a\", 1, \"bGameName\", cache);\n} else {\n\tvar game = client.user.presence.game.type;\n\tswitch(game) {\n\t\tcase 1: var bType = \"Playing\"; break;\n\t\tcase 2: var bType = \"Listening to\"; break;\n\t\tcase 3: var bType = \"Watching\"; break;\n\t\tdefault: var bType = \"No Game Status\"; break;\n\t};\n\tthis.storeValue(client.user.presence.status, 1, \"bType\", cache);\n\tthis.storeValue(client.presence.game.name, 1, \"bGameName\", cache);\n}\n\n//store bots uptime\n\tvar uptime = process.uptime();\n\tvar days = Math.floor((uptime % 31536000) / 86400);\n\tvar hours = Math.floor((uptime % 86400) / 3600);\n\tvar minutes = Math.floor((uptime % 3600) / 60);\n\tvar seconds = Math.round(uptime % 60);\n\tvar botuptime = (days > 0 ? days + \" days, \":\"\") + (hours > 0 ? hours + \" hrs, \":\"\") + (minutes > 0 ? minutes + \" min, \":\"\") + (seconds > 0 ? seconds + \" sec\":\"\");\nthis.storeValue(botuptime, 1, \"botuptime\", cache);\n\n//store list of guilds & count\nvar guildList = client.guilds.cache.array();\nvar botGuilds = \"\";\nfor (var g = 0; g < guildList.length; g++){\n\tbotGuilds += (\"- \" + String(guildList[g].name + \" (\" + guildList[g].memberCount + \" users) - *Joined \" + guildList[g].joinedAt.toLocaleString(\"en-us\", {timeZone: \"America/New_York\", timeZoneName: \"short\", month: \"short\", day: \"2-digit\", year: \"numeric\"}) + \"*\\n\"));\n};\nthis.storeValue(botGuilds, 1, \"botGuilds\", cache);","storage":"0","varName":"","name":"Run Script"},{"title":"","author":"${tempVars(\"bName\")} Bot Info","color":"RANDOM","timestamp":"true","url":"","authorIcon":"${tempVars(\"bAvatar\")}","imageUrl":"","thumbUrl":"${tempVars(\"bAvatar\")}","storage":"1","varName":"e","name":"Create Embed Message"},{"storage":"1","varName":"e","fieldName":"Misc Info","message":"- **# of Commands:** ${this.getDBM().Files.data.commands.length - 1}\n- **# of Events:** ${this.getDBM().Files.data.events.length - 1} \n- **Bot Prefix:** ${this.getDBM().Files.data.settings.tag}\n- **Command Seperator:** ${this.getDBM().Files.data.settings.separator}\n- **Case Sensitive:** ${this.getDBM().Files.data.settings.case}\n- **Bot Owner:** <@${this.getDBM().Files.data.settings. ownerId}>","inline":"0","name":"Add Embed Field"},{"storage":"1","varName":"e","fieldName":"General Information","message":"- **Status**: ${tempVars(\"bStatus\")}\n- **${tempVars(\"bType\")}:** ${tempVars(\"bGameName\")}\n- **Uptime:** ${tempVars(\"botuptime\")}\n- **Ping**: ${Math.round(client.ws.ping)} ms","inline":"0","name":"Add Embed Field"},{"storage":"1","varName":"e","fieldName":"About the Bot","message":"- **Name:** ${tempVars(\"bTag\")}\n- **ID:** ${tempVars(\"bID\"